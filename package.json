{
  "name": "whatsapp-bot-automotiva",
  "version": "2.1.0",
  "description": "Bot de atendimento automatizado para loja automotiva via WhatsApp com IA",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "server": "node src/server.js",
    "dev:server": "nodemon src/server.js",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:server\"",
    "migrate": "node -e \"require('./src/database/connection').migrate()\"",
    "seed": "node src/database/seed.js",
    "logs": "tail -f logs/bot.log",
    "reset": "rm -rf auth/ && npm start"
  },
  "keywords": [
    "whatsapp",
    "bot",
    "automotiva",
    "atendimento",
    "automacao",
    "openai",
    "ia"
  ],
  "author": "Sua Loja Automotiva",
  "license": "MIT",
  "dependencies": {
    "@whiskeysockets/baileys": "^6.7.9",
    "mysql2": "^3.6.5",
    "dotenv": "^16.3.1",
    "pino": "^8.16.2",
    "qrcode-terminal": "^0.12.0",
    "openai": "^4.20.0",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "multer": "^1.4.5-lts.1",
    "qrcode": "^1.5.3",
    "uuid": "^9.0.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "socket.io": "^4.7.2",
    "dayjs": "^1.11.10",
    "node-cache": "^5.1.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
